{
  "manifest_version": 2,
  "name": "Strybok",
  "description":  "Hop between different tools faster!",
  "version": "1.0.1",
  "content_security_policy": "script-src 'self'; object-src 'self'",
  "icons": {
		"128": "images/icon128.png",
		"16": "images/icon16.png"
	},
  "browser_action": {
		"default_icon": "images/icon16.png",
		"default_popup": "html/popup.html",
		"default_tilte": "Strybok"
	},
  "permissions": [
    "activeTab",
    "contextMenus",
    "storage",
    "*://*/"
  ],
  "background": {
    "scripts": ["js/background.js"]
},
"key": "YOUR_KEY"
}
